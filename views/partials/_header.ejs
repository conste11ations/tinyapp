<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-primary justify-content-between">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarText"
      aria-controls="navbarText" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarText">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item">
          <a class="nav-link" href="/urls">My URLs</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/urls/new">Create New URL</a>
        </li>
      </ul>
      <% if (!user) { %>
      <div class="nav-item">
        <form class="form-inline" action="/login" method="GET">
          <button type="submit" class="btn btn-light my-2 my-sm-0">Login</button>
        </form>
      </div>
      <div class="nav-item">
        <form class="form-inline" action="/register" method="GET">
          <button type="submit" class="btn btn-light my-2 my-sm-0">Register</button>
        </form>
      </div>
      <% } else { %>
      <div class="nav-item">
        <form class="form-inline" action="/logout" method="POST">
          <label for="inlineFormInputName2">Logged in as: <%= user.email %> . </label>
          <button type="submit" class="btn btn-light my-2 my-sm-0">Logout</button>
        </form>
      </div>
      <% } %>
    </div>
  </nav>
</header>